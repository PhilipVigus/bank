{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base",
        "airbnb",
        "eslint:recommended",
        "plugin:jasmine/recommended",
        "prettier",
        "prettier/react"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
        "jasmine",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}